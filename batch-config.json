{
    "taskGroups": [
        {
            "taskSpec": {
                "runnables": [
                    {
                        "container": {
                            "imageUri": "us-east4-docker.pkg.dev/rave-367918/mydockerrepo/hello:testing",
                            "options": "--privileged",
                            "volumes": [
                                "/var/lib/nvidia/lib64:/usr/local/nvidia/lib64",
                                "/var/lib/nvidia/bin:/usr/local/nvidia/bin"
                            ]


                        },
                        "environment": {
                            "variables": {
                                "JOBCMD": "echo hello steppy"
                            }
                        }
                    }
                ],
                "volumes": [

                ],
                "computeResource": {
                    "cpuMilli": 4000,
                    "memoryMib": 20000
                }
            },
            "taskCount": 1,
            "parallelism": 1
        }
    ],
    "allocationPolicy": {
        "instances": [
            {
                "installGpuDrivers": true,
                "policy": {
                    "machineType": "n1-standard-8",
                    "accelerators": [
                        {
                            "type": "nvidia-tesla-t4",
                            "count": 1
                        }
                    ]
                }
            }
        ],
        "location": {
            "allowedLocations": [
                "regions/us-east4"
            ]
        }
    },
    "logsPolicy": {
        "destination": "CLOUD_LOGGING"
    }
}
